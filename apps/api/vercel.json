{
  "version": 2,
  "buildCommand": "npm run build",
  "devCommand": "npm run start:dev",
  "installCommand": "npm install && npx prisma generate",
  "framework": null,
  "outputDirectory": "dist",
  "functions": {
    "dist/main.js": {
      "runtime": "@vercel/node@3.0.7",
      "includeFiles": "prisma/**"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/dist/main.js"
    }
  ]
}
